{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@styles/*": ["./styles/*"],
      "@image/*": ["./public/image/*"],
      "@components/*": ["./components/*"],
      "@enumerate/*": ["./enumerate/*"],
      "@hooks/*": ["./hooks/*"],
      "@context/*": ["./context/*"],
      "@utils/*": ["./utils/*"],
      "@assets/*": ["./public/assets/*"],
      "@model/*": ["./model/*"],
      "@api/*": ["./api/*"],
      "@lib/*": ["./lib/*"],
      "@type/*": ["./type/*"],
      "@analytics/*": ["./analytics/*"],
      "@fonts/*": ["./fonts/*"],
      "@constants/*": ["./constant/*"],
      "@store/*": ["./store/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
